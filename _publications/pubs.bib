@Article{Schmidt_WAVANGLETEfficientSupervised_GaRSITo2007,
  author       = {Schmidt, Fr{\' e}d{\' e}ric and Dout{\' e}, Sylvain and Schmitt, Bernard},
  journal      = {Geoscience and Remote Sensing, IEEE Transactions on},
  title        = {WAVANGLET: An Efficient Supervised Classifier for Hyperspectral Images},
  year         = {2007},
  issn         = {0196-2892},
  number       = {5},
  pages        = {1374--1385},
  volume       = {45},
  abstract     = {<para> The new generation of imaging spectrometers onboard planetary missions usually produce hundreds to thousands of images a year, each made up of a thousand to a million spectra with typically several hundred wavelengths. Such huge datasets must be analyzed by efficient yet accurate algorithms. A supervised automatic classification method (hereafter called &ldquo;<emphasis emphasistype="italic">wavanglet</emphasis>&rdquo;) is proposed to identify spectral features and classify images in spectrally homogeneous units. It uses four steps: 1) selection of a library composed of reference spectra; 2) application of a Daubechies wavelet transform to referenced spectra and determination of the wavelet subspace that best separates all referenced spectra; and 3) in this selected subspace, determination of the best threshold on the spectral angle to produce detection masks. This application is focused on the Martian polar regions that present three main types of terrains: <formula formulatype="inline"><tex>$ hbox{H}_{2}hbox{O}$</tex></formula> ice, <formula formulatype="inline"><tex>$hbox{CO}_{2}$</tex></formula> ice, and dust. The wavanglet method is implemented to detect these major compounds on near-infrared hyperspectral images acquired by the OMEGA instrument onboard the Mars Express spacecraft. With an overall accuracy of 89%, wavanglet outperforms two generic methods: band ratio (57% accuracy) and spectral feature fitting (83% accuracy). The quantitative detection limits of wavanglet are also evaluated in terms of abundance for <formula formulatype="inline"><tex>$hbox{H}_{2}hbox{O}$</tex></formula> and <formula formulatype="inline"><tex>$ hbox{CO}_{2}$</tex></formula> ices in order to improve the interpretation of the masks. </para>},
  comment      = {perso&},
  creationdate = {2024-09-30T17:15:39},
  doi          = {10.1109/TGRS.2006.890577},
  file         = {Schmidt_Wavanglet_IEEETGRS2007.pdf:Schmidt_Wavanglet_IEEETGRS2007.pdf:PDF},
  keywords     = {Automatic detection, automatic supervised classification, hyperspectral images, mass treatment, pattern recognition, remote sensing, spectral feature recognition and extraction, wavelet filtering},
  owner        = {Boudoudou},
}

